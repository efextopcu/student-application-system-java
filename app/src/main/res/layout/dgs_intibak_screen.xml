<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="?attr/colorButtonNormal"
        android:visibility="visible"
        tools:context=".MainActivity">

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/stock_photo_dark_empty_black_grey_paper"
            tools:layout_editor_absoluteX="2dp"
            tools:layout_editor_absoluteY="0dp" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="406dp"
            android:layout_height="47dp"
            android:layout_marginTop="24dp"
            android:text="DGS İntibak Başvurusu"
            android:textAlignment="center"
            android:textColor="#DFD5D5"
            android:textSize="34sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView3" />

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="74dp"
            android:layout_height="34dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="244dp"
            android:background="@drawable/_36_1362196_grey_gradient_gradient_grey_png"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar3"
            android:layout_width="73dp"
            android:layout_height="33dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/_36_1362196_grey_gradient_gradient_grey_png"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/toolbar" />

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar4"
            android:layout_width="72dp"
            android:layout_height="34dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/_36_1362196_grey_gradient_gradient_grey_png"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/toolbar3" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="314dp"
            android:layout_height="42dp"
            android:layout_marginTop="20dp"
            android:text="Lütfen gerekli belgeleri sisteme yükleyiniz."
            android:textColor="#DFD5D5"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.505"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="183dp"
            android:layout_height="23dp"
            android:layout_marginTop="244dp"
            android:layout_marginEnd="168dp"
            android:text="1. Transkript Belgesi"
            android:textColor="#DFD5D5"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="183dp"
            android:layout_height="23dp"
            android:layout_marginTop="17dp"
            android:layout_marginEnd="84dp"
            android:text="2.Onaylı Ders içeriği"
            android:textColor="#DFD5D5"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toStartOf="@+id/intibak_onay"
            app:layout_constraintTop_toBottomOf="@+id/toolbar" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="183dp"
            android:layout_height="23dp"
            android:layout_marginTop="13dp"
            android:layout_marginEnd="84dp"
            android:text="3.Yabancı Dil Yeterlilik Belgesi"
            android:textColor="#DFD5D5"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toStartOf="@+id/intibak_dil"
            app:layout_constraintTop_toBottomOf="@+id/toolbar3" />

        <ImageView
            android:id="@+id/cap_puan"
            android:layout_width="35dp"
            android:layout_height="34dp"
            android:layout_marginTop="244dp"
            android:layout_marginEnd="48dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@android:drawable/ic_menu_upload" />

        <ImageView
            android:id="@+id/intibak_onay"
            android:layout_width="35dp"
            android:layout_height="34dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="48dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cap_puan"
            app:srcCompat="@android:drawable/ic_menu_upload" />

        <ImageView
            android:id="@+id/intibak_dil"
            android:layout_width="35dp"
            android:layout_height="34dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="48dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/intibak_onay"
            app:srcCompat="@android:drawable/ic_menu_upload" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="70dp"
            android:layout_height="68dp"
            android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/unnamed" />

        <Button
            android:id="@+id/showDatePickerDialog"
            android:layout_width="188dp"
            android:layout_height="52dp"
            android:layout_marginBottom="56dp"
            android:backgroundTint="@android:color/darker_gray"
            android:text="Devam Et"
            app:iconTint="#A13232"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>